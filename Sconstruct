env = SConscript("godot-cpp/SConstruct")

env.Append(CPPPATH = ["src/"])

sources = Glob("src/*.cpp") + Glob("src/OrangeCharacter/*.cpp") + Glob("src/EnemMob/*.cpp") + Glob("src/MainNode/*.cpp")

library = env.SharedLibrary("demo/bin/libMainNode{}{}".format(env["suffix"],env["SHLIBSUFFIX"]), source= sources)

Default(library)